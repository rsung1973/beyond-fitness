
@using System.IO
@using System.Linq.Expressions

@using WebHome.Helper
@using WebHome.Models.Locale
@using WebHome.Models.ViewModel
@using WebHome.Models.DataEntity
@using WebHome.Controllers
@using Newtonsoft.Json

@{
    ModelStateDictionary _modelState;
    ModelSource<UserProfile> models;
    UserProfile _model;
    _modelState = (ModelStateDictionary)ViewBag.ModelState;
    models = ((SampleController<UserProfile>)ViewContext.Controller).DataSource;
    _model = (UserProfile)this.Model;
}

@if (_model.IsAssistant() || _model.IsOfficer() || _model.IsServitor())
{
    Html.RenderPartial("~/Views/ContractConsole/Module/AboutNewContractsByOfficer.cshtml", _model);
}
else if (_model.IsManager() || _model.IsViceManager())
{
    Html.RenderPartial("~/Views/ContractConsole/Module/AboutNewContractsByBranch.cshtml", _model);
}
else
{
    ViewBag.EnableToCreate = false;
    Html.RenderPartial("~/Views/ContractConsole/Module/AboutNewContractsByCoach.cshtml", _model);
}



