
@using System.IO
@using System.Linq.Expressions

@using WebHome.Helper
@using WebHome.Models.Locale
@using WebHome.Models.ViewModel
@using WebHome.Models.DataEntity
@using WebHome.Controllers
@using Microsoft.AspNetCore.Mvc.ModelBinding

@using Newtonsoft.Json
@{

    String _dialogID = $"contractDetails{DateTime.Now.Ticks}";
    ModelSource<UserProfile> models;
    ModelStateDictionary _modelState;
    IQueryable<RegisterLesson> _model;

    models = (ModelSource<UserProfile>)ViewContext.HttpContext.Items["Models"];;
    _modelState = ViewContext.ModelState;
    _model = (IQueryable<RegisterLesson>)this.Model;
}
<div class="modal fade" id="@(_dialogID)" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <a class="closebutton" data-dismiss="modal"></a>
            <div class="card white">
                <div class="body">
                    <div class="table-responsive">
                        @{
                            await Html.RenderPartialAsync("~/Views/ConsoleHome/CourseContract/RemainedLessonList2022.cshtml", _model);
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
    @{ await Html.RenderPartialAsync("~/Views/ConsoleHome/Shared/BSModalScript.cshtml", model: _dialogID);}

</div>

