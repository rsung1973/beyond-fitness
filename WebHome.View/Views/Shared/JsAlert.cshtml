@using System.IO
@using System.Linq.Expressions
@using System.Web

@using WebHome.Helper
@using WebHome.Models.Locale
@using WebHome.Models.ViewModel
@using WebHome.Models.DataEntity
@using WebHome.Controllers
@using Microsoft.AspNetCore.Mvc.ModelBinding
@{
    QueryViewModel _viewModel = ViewBag.ViewModel as QueryViewModel;

    String _message;
    _message = (String)this.Model ?? (String)ViewBag.Message ?? _viewModel?.Message;

}
@if (_message != null)
{
    <script>
        alert('@Html.Raw(HttpUtility.JavaScriptStringEncode(_message))');
        if (typeof (hideLoading) === 'function') {
            hideLoading();
        }
    </script>
}
@if (ViewBag.GoBack == true)
{
    <script>
        window.history.go(-1);
    </script>
}
@if (ViewBag.CloseWindow == true)
{
    <script>
        window.close();
    </script>
}


