    _viewModel = ViewBag.ViewModel as PaymentQueryViewModel;
}
@{ await Html.RenderPartialAsync("~/Views/PaymentConsole/Module/PaymentItemsList.cshtml", _model);}

<script>

    $(function () {
        if ($global.queryCallback) {
            $global.queryCallback();
        }
    });

</script>





