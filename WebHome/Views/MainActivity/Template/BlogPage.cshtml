<body>
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-T5GQNTV"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
    <!-- //Main Nav -->
    <header id="main-nav">
        <div class="container">
            <div class="row">
                <div class="col-md-2 col-sm-2 col-xs-12 text-center">
                    <a href="javascript:bindTrackingCode('圖文分類', '@(Url.Action("BlogGrid", "MainActivity"))', null)" class="logo"><img src="images/logo.png" alt="BeyondFitness"></a>
                </div>
                <div class="col-md-10 col-sm-10 col-xs-12">
                    <ul class="left nav-active">
                        <li class="pull-right">
                            <a href="javascript:bindTrackingCode('官方網站', '@(VirtualPathUtility.ToAbsolute("~/MainActivity/Index"))', null)" class="btn btn-default btn-round">@Html.Raw(NamingItem.BlogBack)</a>
                        </li>
                    </ul>
                    <script>
                        function changeLanguage(lang) {
                            $.post('@(Url.Action("ChangeLanguate", "MainActivity"))', { 'lang': lang }, function (data) {
                                if ($.isPlainObject(data)) {
                                    if (data.result) {
                                        window.location.reload();
                                    }
                                } else {
                                    $(data).appendTo($('body'));
                                }
                            });
                        }
                    </script>
                </div>
            </div>
        </div>
    </header>
    <!-- Main Content -->
    @RenderBody()
    @RenderSection("TailPageJavaScriptInclude", false)

    <!-- // Footer -->
    <footer id="footer">
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                </div>
            </div>
        </div>
    </footer>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    @*<script async src="https://www.googletagmanager.com/gtm.js?id=GTM-T5GQNTV"></script>*@
    <script async src="https://www.googletagmanager.com/gtag/js?id=@(WebHome.Startup.Properties["GA_ID_Frontend"])"></script>
    <script type="text/javascript">
@*window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', '@Startup.Properties["GA_ID_Frontend"]');*@


        this.bindTrackingCode = function (gaEventName, linkURL, linkType) {
            if (gaEventName && typeof gaEventName != "undefined") {
                gtag("event", gaEventName, { event_category: "連結點擊", event_label: "知識分享" });
            }

            if (linkURL && typeof linkURL != "undefined") {
                if (linkType && linkType == '_blank') {
                    window.open(linkURL);
                } else {
                    window.location.href = linkURL;
                }
            }
        }
    </script>
</body>
</html>
